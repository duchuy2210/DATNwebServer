<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <script src="js/index.js"></script>
  </head>
  <body>
    <!-- Nút nhấn chạy động cơ -->
    <button id="btt_RUN_M1" onclick="socket.emit('Client-send-cmdM1', true);">
      RUN MOTOR 1
    </button>
    <!-- Nút nhấn dừng động cơ -->
    <button id="btt_STOP_M1" onclick="socket.emit('Client-send-cmdM1', false);">
      STOP MOTOR 1
    </button>
    <form class="mt-3">
      <div class="mb-3">
        <label for="Bool" class="form-label">Tag dạng Bool:</label>
        <input class="form-control" id="Bool" disabled />
      </div>
      <div class="mb-3">
        <label for="Byte" class="form-label">Tag dạng Byte:</label>
        <input class="form-control" id="Byte" disabled />
      </div>
      <div class="mb-3">
        <label for="Integer" class="form-label">Tag dạng Integer:</label>
        <input class="form-control" id="Integer" disabled />
      </div>
      <div class="mb-3">
        <label for="Real" class="form-label">Tag dạng Real:</label>
        <input class="form-control" id="Real" disabled />
      </div>
      <div class="mb-3">
        <label for="String" class="form-label">Tag dạng String:</label>
        <input class="form-control" id="String" disabled />
      </div>
      <!-- <button type="submit" class="btn btn-primary">Submit</button> -->
    </form>
  </body>
</html>
